---
import ResumeBackEnd from "../../components/ResumeBackEnd.astro";
import ResumeFrontEnd from "../../components/ResumeFrontEnd.astro";
import Skills from "../../components/Skills.astro";
import Layout from "../../layouts/Layout.astro";
---
<Layout title="Resume: Express" />
<container class="grid grid-cols-3 justify-items-center">
<ResumeFrontEnd />
  <Skills 
  focused= "Express"
  projects= {[
    {
      name: "Squid Chatter",
      description: "A messanger app utilizing Firebase and React. - functioning but in progress",
      updated: "9-14-2022",
      href: "https://git.heroku.com/squid-chatter.git",
      github: "https://github.com/BihonAnon/realtime-chatroom-react-firebase"
    },
  ]}
  />
  <ResumeBackEnd />
</container>
<style>
    img {
        width: 90px;
        height: 18px;
        object-fit: cover;
    }
</style>