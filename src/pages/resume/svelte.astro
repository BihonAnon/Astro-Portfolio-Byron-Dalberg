---
import ResumeBackEnd from "../../components/ResumeBackEnd.astro";
import ResumeFrontEnd from "../../components/ResumeFrontEnd.astro";
import Skills from "../../components/Skills.astro";
import Layout from "../../layouts/Layout.astro";
---
<Layout title="Resume: Svelte" />
<container class="grid grid-cols-3 justify-items-center">
<ResumeFrontEnd />
  <Skills 
  focused= "Svelte"
  projects= {[
    {
      name: "Prototype-Messanger",
      description: "A Message built using Svelte, a new framework that is similar to React and Vue. It uses a REST API to store messages and users. It also uses a websocket to send messages in real time.",
      updated: "9-12-2022",
      href: "#tbd",
      github: "https://github.com/BihonAnon/Prototype-Messager"
    },
  ]}
  />
  <ResumeBackEnd />
</container>
<style>
    img {
        width: 90px;
        height: 18px;
        object-fit: cover;
    }
</style>